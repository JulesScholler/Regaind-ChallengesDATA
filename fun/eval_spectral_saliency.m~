function [s]=eval_spectral_saliency(im)

im=double(rgb2gray(im)/255);
x=[1 round(size(im_HSV,1)*1/3) round(size(im_HSV,1)*2/3) size(im_HSV,1)];
y=[1 round(size(im_HSV,2)*1/3) round(size(im_HSV,2)*2/3) size(im_HSV,2)];

for i=1:3
    for j=1:3
        ims{i}=im(x(i):x(i+1),y(j):y(j+1));
    end
end

n=5;
h=1/n^2*ones(n);
imconv=conv2(im,h,'same');
L=fft2(im);
Llog=fftshift(log(1+abs(L)));
A=fft2(imconv);
Alog=fftshift(log(1+abs(A)));
d=sqrt((Llog-Alog).^2)/length(im(:));



pause